      <string notr="true">/* QComboBox base style */
QComboBox {
    border: 1px solid #377ab7; /* Border color matches blue */
    border-radius: 4px; /* Rounded corners */
    padding: 2px;
    background-color: #f2f2f2; /* Light gray background */
    color: black; /* Text color */
}

/* Style the drop-down button */
QComboBox::drop-down {
    border: none; /* No border around the drop-down button */
    background-color: #377ab7; /* Blue background for the button */
    width: 20px; /* Width of the drop-down button */
}

/* QComboBox down arrow style */
QComboBox::down-arrow {
    image: url(:/qt-project.org/styles/commonstyle/images/arrow_down.png); /* Default arrow image */
    width: 12px; /* Width of the down arrow */
    height: 12px; /* Height of the down arrow */
}

/* Style for QAbstractItemView (dropdown list) */
QComboBox QAbstractItemView {
    border: 1px solid #377ab7; /* Border color matches blue */
    background-color: #f2f2f2; /* Light gray background for dropdown */
    selection-background-color: #2b6698; /* Darker blue for selection */
    selection-color: white; /* White text color for selected items */
    outline: none; /* No outline */
    color: black; /* Text color for non-highlighted items */
}

/* Style each item in the dropdown list */
QComboBox QAbstractItemView::item {
    padding: 4px 8px; /* Add padding to each item */
    color: black; /* Ensure text color contrasts with background */
}

/* Hover effect for items in the dropdown list */
QComboBox QAbstractItemView::item:hover {
    background-color: #377ab7; /* Blue background on hover */
    color: white; /* White text color */
}

/* Style for selected items in the dropdown list */
QComboBox QAbstractItemView::item:selected {
    background-color: #2b6698; /* Darker blue for selection */
    color: white; /* White text color */
}
