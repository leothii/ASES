    height: 16px; /* Height of the checkbox indicator */
}

/* Style for when the QCheckBox is checked */
QCheckBox::indicator:checked {
    background-color: #377ab7; /* Blue background color when checked */
    border: 1px solid #377ab7; /* Border color matches the blue */
    border-radius: 4px; /* Rounded corners */
}

QCheckBox::indicator:unchecked {
    border: 1px solid gray; /* Border color for unchecked state */
    border-radius: 4px; /* Rounded corners */
}

/* Style for the checkmark when QCheckBox is checked */
QCheckBox::indicator:checked::after {
    content: &quot;&quot;; /* Required for the checkmark */
