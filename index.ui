/* Style for the scrollbar */
QScrollBar:vertical {
    border: none; /* No border around scrollbar */
    background-color: #d0d0d0; /* A little darker than the background color */
    width: 12px; /* Width of the scrollbar */
    margin: 0; /* No margin */
}

/* Handle style for the vertical scrollbar */
QScrollBar::handle:vertical {
    background-color: #b0b0b0; /* Handle color, darker than scrollbar */
    border-radius: 6px; /* Rounded corners */
    min-height: 20px; /* Minimum height of the handle */
}

/* Handle style when hovering over the scrollbar */
QScrollBar::handle:vertical:hover {
    background-color: #a0a0a0; /* Slightly darker handle when hovered */
}

/* Style for the up and down buttons of the scrollbar */
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    border: none; /* No border */
    background-color: transparent; /* Transparent background */
    height: 0; /* No height */
}

/* Style for the up and down arrows */
QScrollBar::add-line:vertical:hover,
QScrollBar::sub-line:vertical:hover {
    background: transparent; /* Transparent background on hover */
}

/* Style for the empty space around the handle */
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background-color: #d0d0d0; /* Background color to match the scrollbar */
}
